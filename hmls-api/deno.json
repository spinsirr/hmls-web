{
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-env --allow-read main.ts",
    "start": "deno run --allow-net --allow-env --allow-read main.ts",
    "db:generate": "deno run -A npm:drizzle-kit generate",
    "db:push": "deno run -A npm:drizzle-kit push",
    "db:seed": "deno run --allow-net --allow-env --allow-read db/seed.ts"
  },
  "imports": {
    "@corespeed/zypher": "jsr:@corespeed/zypher",
    "@corespeed/zypher/": "jsr:@corespeed/zypher/",
    "hono": "npm:hono@^4",
    "hono/cors": "npm:hono@^4/cors",
    "hono/logger": "npm:hono@^4/logger",
    "drizzle-orm": "npm:drizzle-orm",
    "drizzle-orm/pg-core": "npm:drizzle-orm/pg-core",
    "drizzle-orm/postgres-js": "npm:drizzle-orm/postgres-js",
    "postgres": "npm:postgres",
    "zod": "npm:zod",
    "rxjs-for-await": "npm:rxjs-for-await"
  }
}
